{
  "name": "gatsby-plugin-css-literal-loader",
  "version": "0.0.0-development",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/4Catalyzer/gatsby-plugin-css-literal-loader.git"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "css-in-js",
    "css",
    "less",
    "sass"
  ],
  "author": "4Catalyzer",
  "license": "MIT",
  "scripts": {
    "tdd": "jest --watch",
    "test": "npm run lint",
    "testonly": "jest",
    "lint": "eslint . && prettier --list-different --ignore-path .eslintignore '**/*.{json,css,md}'",
    "format": "eslint . --fix && prettier --write --ignore-path .eslintignore '**/*.{json,css,md}'",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 79,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write --ignore-path .eslintignore",
      "git add"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/test"
    ],
    "testEnvironment": "node"
  },
  "release": {
    "extends": [
      "@4c/semantic-release-config"
    ]
  },
  "peerDependencies": {
    "gatsby": ">= 2.0.0-0"
  },
  "devDependencies": {
    "@4c/semantic-release-config": "^1.0.2",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.16.0",
    "eslint-config-4catalyzer": "^0.4.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.7.0",
    "eslint-plugin-prettier": "^2.5.0",
    "husky": "^0.14.3",
    "jest": "^23.4.2",
    "lint-staged": "^7.1.0",
    "prettier": "^1.10.2",
    "semantic-release": "^15.9.2",
    "travis-deploy-once": "^5.0.1"
  },
  "dependencies": {
    "css-literal-loader": "^0.5.2"
  }
}
